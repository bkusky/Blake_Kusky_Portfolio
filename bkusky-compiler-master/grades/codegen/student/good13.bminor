// post add and sub

main: function integer () = {
    value: integer = 5;

    old_value: integer = value++;
    print "----adding----\n";

    print "old --> ", old_value, "\n";
    print "current --> ", value, "\n";

    print "---subtracting----\n";
    
    old_value = value--;
    
    print "old --> ", old_value, "\n";
    print "current --> ", value, "\n";

    return 0;
}